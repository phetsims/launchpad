{
  "name": "phettest",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/phettest.git"
  },
  "type": "module",
  "scripts": {
    "server": "bash ../perennial/bin/sage run js/server/launchpad-server.ts",
    "local-server": "bash ../perennial/bin/sage run js/server/launchpad-server.ts --autoUpdate=false --autoBuild=false --checkClean=true",
    "start-client": "NODE_ENV=development npx esbuild js/client/client.ts index.html --loader:.html=copy --bundle --outdir=dist --serve=8081",
    "build-client": "NODE_ENV=production npx esbuild js/client/client.ts index.html --loader:.html=copy --bundle --outdir=dist"
  },
  "dependencies": {
    "fuzzysort": "^3.1.0",
    "moment": "^2.30.1",
    "scenerystack": "^3.0.0"
  },
  "devDependencies": {
    "basic-auth": "^2.0.1",
    "esbuild": "~0.25.0",
    "grunt": "~1.5.3",
    "express": "^5.1.0",
    "globals": "^15.14.0",
    "nopt": "~4.0.1",
    "octokit": "^5.0.3",
    "p-limit": "^7.1.1",
    "serve-index": "^1.9.1",
    "typescript": "~5.7.2",
    "@types/basic-auth": "^1.1.8",
    "@types/express": "~5.0.1",
    "@types/node": "^24.7.1",
    "@types/nopt": "~3.0.32",
    "@types/serve-index": "^1.9.4"
  }
}