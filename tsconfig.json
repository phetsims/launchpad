{
  "extends": "../perennial-alias/tsconfig/tsconfig-node.json",
  "references": [
    {
      "path": "../chipper/tsconfig/buildtools/tsconfig.json"
    }
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["ES2020", "DOM"]
  },
  "include": ["js/client", "js/types"]
}
